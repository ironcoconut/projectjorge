{
  "header": {
    "title": "Case Worker",
    "subtitle": "Mary's Charity"
  },
  "footer": {
    "exclude": "Volunteer"
  },
  "form": {
    "submit_url": "api/volunteer/marys-charity/signup",
    "elements": [
      {
        "label": "Your full name",
        "attributes": {
          "type": "text",
          "placeholder": "Name",
          "ref": "name"
        }
      },
      {
        "label": "Your email address.",
        "attributes": {
          "type": "text", 
          "placeholder": "Email", 
          "ref": "email"
        }
      },
      {
        "label": "Your phone number.",
        "attributes": {
          "type": "text", 
          "placeholder": "Phone", 
          "ref": "phone"
        }
      }
    ]
  }
}
